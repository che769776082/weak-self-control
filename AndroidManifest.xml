<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.weakselfcontrol"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/name"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
         <activity
            android:name="com.example.weakselfcontrol.Tabhost"
            android:label="@string/app_name" >
             <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.weakselfcontrol.FilterApplication"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.weakselfcontrol.MainActivity"
            android:label="@string/app_name" >
        </activity>
         <activity
            android:name="com.example.weakselfcontrol.Help"
            android:label="@string/app_name" >
        </activity>
          <service
            android:name="com.example.weakselfcontrol.LockService"
            android:label="@string/app_name" >
        </service>
          <activity
            android:name="com.example.weakselfcontrol.Settings"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.weakselfcontrol.Warning"
            android:label="@string/app_name" >
        </activity>
    </application>
<uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES"/>
<uses-permission android:name="android.permission.GET_TASKS"/>
<uses-permission android:name="android.permission.RESTART_PACKAGES"/>
</manifest>
